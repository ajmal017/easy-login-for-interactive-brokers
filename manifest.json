
  {
    "name": "Easy Login for Interactive Brokers",
    "description" : "Base Level Extension",
    "manifest_version": 2,
    "version": "1.1",
   
    "permissions": ["activeTab", "storage"],
    "browser_action": {
      "default_icon": "IB.png",
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://zh.wgw.interactivebrokers.com/webtrader/servlet/login", "https://www.interactivebrokers.com.hk/sso/Login"],
        "js": ["popup.js"]
      }
    ],
    "background": {
      "scripts": ["background.js"]
    }
  }